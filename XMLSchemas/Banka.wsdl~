			<wsdl:input message="ws:zahtevZaIzvodRequest" />
		</wsdl:operation>
	</wsdl:portType>

	
	<!-- WSDL binding -->

	<wsdl:binding name="BankaServiceBinding" type="ws:BankaServiceMessages">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
